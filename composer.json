{
    "name": "leandro980/skebby-bundle",
    "description": "A bundle to send SMS through Skebby service",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Leandro Luccerini",
            "email": "leandro.luccerini@gmail.com"
        }
    ],
    "keywords" : [
        "symfony sms skebby gateway"
    ],
    "require" : {
        "ext-curl": "*",
        "ext-json": "*",
        "php": ">=7",
        "symfony/framework-bundle": "^2.7 || ^3.0 || ^4.0",
        "doctrine/doctrine-bundle": "*",
        "giggsey/libphonenumber-for-php": "^8.10",
        "guzzlehttp/guzzle": "^6.3",
        "karriere/json-decoder": "^2.2",
        "instasent/sms-counter-php": "^0.4.2"
    },
    "autoload" : {
        "psr-4": { "Szopen\\SkebbyBundle\\": "src/" }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0"
        }
    }
}
